<div id ="display_list">
  <h1>Lists for <%= @user.name %></h1>
  <form action="/user/<%= @user.id %>/list" method="GET">
    <input type="submit" id="create_list_button" value="Create New List">
  </form>
  <table>
    <tr>
      <th>Name</th>
      <th>Last Updated</th>
      <th>Status</th>
    </tr>
    <% @user.lists.each do |list| %>
      <tr>
      <td><a href="/user/<%= @user.id %>/list/<%= list.id %>" class="remote"><%= list.name %></a></td>
      <td><%= list.updated_at.month %>/<%= list.updated_at.day %>/<%= list.updated_at.year %></td>
      <td></td>
      </tr>
    <% end %>
  </table>
</div>